*color10: #C7B9AC

! yellow
*color3:  #D4BAAD
*color11: #D4BAAD

! blue
*color4:  #C6BABA
*color12: #C6BABA

! magenta
*color5:  #D4BCBC
*color13: #D4BCBC

! cyan
*color6:  #B9B9B9
*color14: #B9B9B9

! white
*color7:  #FCFCFC
*color15: #FCFCFC
