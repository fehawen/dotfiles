! vim:ft=xdefaults:

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

xterm*termName: xterm-256color
xterm*scaleHeight: 1.1
xterm*geometry: 52x12
xterm*internalBorder: 120
xterm*borderWidth: 0

xterm*utf8: true
xterm*scrollBar: false
xterm*cursorBlink: false
xterm*selectToClipboard: true
xterm*trimSelection: true
xterm*renderFont: true
xterm*wideChars: true
xterm*visualBell: false

xterm.vt100.translations: #override \n\
    Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
    Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

xterm*faceName: xft:JetBrains Mono:pixelsize=34:antialias=true
xterm*faceNameDoublesize: xft:JetBrains Mono:pixelsize=34:antialias=true
xterm*wideFont: xft:JetBrains Mono:pixelsize=34:antialias=true

*background: #1e282c
*foreground: #b2bebf
*cursorColor: #b2bebf

*color0:  #1e282c
*color8:  #566166

*color1:  #b56666
*color9:  #b56666

*color2:  #779687
*color10: #779687

*color3:  #b98675
*color11: #b98675

*color4:  #5e828f
*color12: #5e828f

*color5:  #b48291
*color13: #b48291

*color6:  #7ab1b3
*color14: #7ab1b3

*color7:  #b2bebf
*color15: #b2bebf
