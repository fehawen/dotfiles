! vim:ft=xdefaults:

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

xterm*termName: xterm-256color
xterm*scaleHeight: 1.0
xterm*geometry: 52x12
xterm*internalBorder: 120
xterm*borderWidth: 0

xterm*utf8: true
xterm*scrollBar: false
xterm*cursorBlink: false
xterm*selectToClipboard: true
xterm*trimSelection: true
xterm*renderFont: true
xterm*wideChars: true
xterm*visualBell: false

xterm.vt100.translations: #override \n\
    Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
    Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

xterm*faceName: xft:Roboto Mono:pixelsize=34:antialias=true
xterm*faceNameDoublesize: xft:Roboto Mono:pixelsize=34:antialias=true
xterm*wideFont: xft:Roboto Mono:pixelsize=34:antialias=true

*background: #1c1f27
*foreground: #e8e8e8
*cursorColor: #e8e8e8

*color0:  #1c1f27
*color8:  #6e717b

*color1:  #ed858a
*color9:  #ed858a

*color2:  #85edb3
*color10: #85edb3

*color3:  #eda685
*color11: #eda685

*color4:  #85b8ed
*color12: #85b8ed

*color5:  #ca85ed
*color13: #ca85ed

*color6:  #85e8ed
*color14: #85e8ed

*color7:  #e8e8e8
*color15: #e8e8e8
